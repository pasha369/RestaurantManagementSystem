        <div class="col-lg-7 col-sm-6">
            <div class="service-block service-block-sea service-or">
                <div class="service-bg"></div>
                <i class="icon-custom icon-color-light rounded-x fa fa-check"></i>
                <h2 class="heading-md">@Resources.Your_Payment_was_successful </h2>
                <p>@Resources.Thank_you_for_your_payment_Your_transaction_has_been_completed</p>
            </div>
        </div>
    </div>

@model List<List<DataModel.Model.Country>>

@{
    Layout = null;
}
<div class="container content-sm">
@foreach(var row in Model)
{
    <div class="row ">
    @foreach (var item in row)
    {
              <div class="col-md-3 col-xs-8">
                <div class="view view-tenth">
                    <img class="img-responsive" src="//placehold.it/400x200" alt="">
                    <div class="mask">
                        <h2>@item.Name</h2>
                        <a href='@Url.Action("GetRstByCountry", new {@country=item.Name})' class="info">Get</a>
                    </div>
                </div>
            </div>
    }
    </div>   
    
}

</div>